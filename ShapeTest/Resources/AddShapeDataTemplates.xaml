<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <DataTemplate x:Key="SquareButtonDataTemplate">
        <Rectangle Width="20" Height="20" Fill="Black" ></Rectangle>
    </DataTemplate>

    <DataTemplate x:Key="RectangleButtonDataTemplate">
        <Rectangle Width="24" Height="18" Fill="Black" />
    </DataTemplate>

    <DataTemplate x:Key="CircleButtonDataTemplate">
        <Ellipse Width="20" Height="20" Fill="Black" ></Ellipse>
    </DataTemplate>

    <DataTemplate x:Key="TriangleButtonDataTemplate">
        <Polygon  Points="0,20 20,20 10,0" Fill="Black"  />
    </DataTemplate>

    <DataTemplate x:Key="UnknownButtonDataTemplate">
        <TextBlock Text="?" FontSize="20" />
    </DataTemplate>
    
</ResourceDictionary>